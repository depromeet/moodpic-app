{
  "expo": {
    "name": "moodpic-app",
    "slug": "moodpic-app",
    "owner": "depromeet_5team",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "kr.moodpic.www"
    },
    "android": {
      "versionCode": 2,
      "package": "com.depromeet_5team.moodpicapp",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions" : []
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
